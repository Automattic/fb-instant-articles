    {
        "class": "ImageRule",
        "selector" : "figure",
        "properties" : {
            "image.url" : {
                "type" : "string",
                "selector" : "img",
                "attribute": "src"
            }
        }
    }, {
        "class": "VideoRule",
        "selector" : "div.wp-video",
