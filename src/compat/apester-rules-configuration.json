{
  "rules": [{
    "class": "IgnoreRule",
    "selector": "//script[contains(@src,'apester')]"
  }, {
    "class": "InteractiveRule",
    "selector": "div.apester-media",
    "properties": {
      "interactive.iframe": {
        "type": "multiple",
        "children": [{
          "type": "fragment",
          "fragment": "<script type='text/javascript' src='//static.apester.com/js/sdk/latest/apester-sdk.min.js'></script>"
        }, {
          "type": "element",
          "selector": "div.apester-media"
        }]
      }
    }
  }]
}